{
  "kind": "Deployment",
  "apiVersion": "extensions/v1beta1",
  "metadata": {
    "name": "web",
    "creationTimestamp": null
  },
  "spec": {
    "replicas": 1,
    "template": {
      "metadata": {
        "creationTimestamp": null,
        "labels": {
          "service": "web"
        }
      },
      "spec": {
        "volumes": [
          {
            "name": "storage-0",
            "persistentVolumeClaim": {
              "claimName": "storage-0"
            }
          }
        ],
        "containers": [
          {
            "name": "web",
            "image": "python:2-slim",
            "ports": [
              {
                "containerPort": 5000,
                "protocol": "TCP"
              }
            ],
            "env": [
              {
                "name": "C_FORCE_ROOT",
                "value": "true"
              },
              {
                "name": "DATABASE_URL",
                "value": "postgresql://postgres:test@postgres:5432/opev"
              },
              {
                "name": "REDIS_URL",
                "value": "redis://redis:6379/0"
              },
              {
                "name": "SERVER_NAME"
              }
            ],
            "resources": {},
            "volumeMounts": [
              {
                "name": "storage-0",
                "mountPath": "/data/open_event/static"
              }
            ]
          }
        ],
        "restartPolicy": "Always"
      }
    },
    "strategy": {}
  },
  "status": {}
}
