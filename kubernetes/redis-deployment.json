{
    "kind": "Deployment",
    "apiVersion": "extensions/v1beta1",
    "metadata": {
        "name": "redis",
        "creationTimestamp": null
    },
    "spec": {
        "replicas": 1,
        "template": {
            "metadata": {
                "creationTimestamp": null,
                "labels": {
                    "service": "redis"
                }
            },
            "spec": {
                "volumes": [
                    {
                        "name": "redis-0",
                        "persistentVolumeClaim": {
                            "claimName": "redis-0"
                        }
                    }
                ],
                "containers": [
                    {
                        "name": "redis",
                        "image": "redis:3-alpine",
                        "args": [
                            "redis-server"
                        ],
                        "ports": [
                            {
                                "containerPort": 6379,
                                "protocol": "TCP"
                            }
                        ],
                        "resources": {},
                        "volumeMounts": [
                            {
                                "name": "redis-0",
                                "mountPath": "/var/lib/redis/data"
                            }
                        ]
                    }
                ],
                "restartPolicy": "Always"
            }
        },
        "strategy": {}
    },
    "status": {}
}
